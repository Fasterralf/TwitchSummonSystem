{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://0.0.0.0:5000",
  "Twitch": {
    "ClientId": "#{TWITCH_CLIENT_ID}#",
    "ClientSecret": "#{TWITCH_CLIENT_SECRET}#",
    "AccessToken": "#{TWITCH_ACCESS_TOKEN}#",
    "RefreshToken": "#{TWITCH_REFRESH_TOKEN}#",
    "ChannelId": "#{TWITCH_CHANNEL_ID}#",
    "ChannelName": "#{TWITCH_CHANNEL_NAME}#",
    "SummonRewardName": "#{TWITCH_SUMMON_REWARD_NAME}#",
    "BotUsername": "#{TWITCH_BOT_USERNAME}#",
    "BotClientId": "#{TWITCH_BOT_CLIENT_ID}#",
    "BotClientSecret": "#{TWITCH_BOT_CLIENT_SECRET}#",
    "ChatOAuthToken": "#{TWITCH_CHAT_OAUTH_TOKEN}#",
    "ChatRefreshToken": "#{TWITCH_CHAT_REFRESH_TOKEN}#"
  },
  "Discord": {
    "WebhookUrl": "#{DISCORD_WEBHOOK_URL}#",
    "ErrorWebhookUrl": "#{DISCORD_ERROR_WEBHOOK_URL}#"
  }
}
